#!/bin/sh

# Normal colors
black="\e[30m"
red="\e[31m"
green="\e[32m"
yellow="\e[33m"
blue="\e[34m"
magenta="\e[35m"
cyan="\e[36m"
white="\e[37m"
grey="\e[37m"

# Bright colors
lblack="\e[30m"
lred="\e[1;31m"
lgreen="\e[1;32m"
lyellow="\e[1;33m"
lblue="\e[1;34m"
lmagenta="\e[1;35m"
lcyan="\e[1;36m"
lwhite="\e[1;37m"
lgrey="\e[1;30m"

reset="\e[0m"
bold="\e[1m"

echo -e "\n${red}███ ${green}███ ${yellow}███ ${blue}███ ${magenta}███ ${cyan}███ ${white}███${reset}"
echo -e "${red}███ ${green}███ ${yellow}███ ${blue}███ ${magenta}███ ${cyan}███ ${white}███${reset}"
echo -e "${lred}▀▀▀ ${lgreen}▀▀▀ ${lyellow}▀▀▀ ${lblue}▀▀▀ ${lmagenta}▀▀▀ ${lcyan}▀▀▀ ${lwhite}▀▀▀${reset}\n"
